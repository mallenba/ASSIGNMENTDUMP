import java.util.Scanner;
import java.util.ArrayList;

public class Exercise_11_10 {

    public class MyStack {
      private ArrayList<Object> list = new ArrayList<>();

      Object objectInheriter = new Object(1);
      
      public Object extender() {
    	    return ArrayList.push(objectInheriter);
    	  }
      
      
      public boolean isEmpty() {
        return list.isEmpty();
      }

      public int getSize() {
        return list.size();
      }

      public Object peek() {
        return list.get(getSize() - 1);
      }

      public Object pop() {
        Object o = list.get(getSize() - 1);
        list.remove(getSize() - 1);
        return o;
      }
      


      public void push(Object o) {
        list.add(o);
      }



      public String toString() {
        return "stack: " + list.toString();
      }
    }



}

public static void main(String[] args) {
	Scanner input = new Scanner(System.in);
	
	MyStack stack = new MyStack();
	
    System.out.print("Enter a sequence of 4 numbers: ");
    //0 basically is like the confirm button
    Integer number = input.nextInt();
    while (number.intValue() != 0) {
        stack.push(number);
        stack.push(input.next());
    }
} 
}
